{
    name: "newism/twig-extensions-bundle",
    type: "symfony-bundle",
    description: "Useful Twig extensions for your Symfony2 project.",
    keywords: ["symfony2", "twig", "extensions"],
    homepage: "https://github.com/newism/TwigExtensionsBundle",
    authors: [{
        name: "Leevi Graham",
        email: "leevi@newism.com.au"
    }, {
        name: "Symfony Community",
        homepage: "https://github.com/newism/TwigExtensionsBundle/contributors"
    }],
    require: {
        php: ">=5.3.2",
        symfony / twig - bundle: "2.0.*"
    },
    "autoload": {
        "psr-0": { "NSM\\TwigExtensionsBundle": "" }
    },
    target - dir: "NSM/TwigExtensionsBundle"
}